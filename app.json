{
  "expo": {
    "name": "MAURIPLAY",
    "slug": "mauriplay",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "icon": "https://i.postimg.cc/QdBvRCw7/oiljoilk.png",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mauriplay.app",
      "icon": "https://i.postimg.cc/QdBvRCw7/oiljoilk.png"
    },
    "android": {
      "package": "com.mauriplay.app",
      "adaptiveIcon": {
        "foregroundImage": "https://i.postimg.cc/QdBvRCw7/oiljoilk.png",
        "backgroundColor": "#0f0f16"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "https://i.postimg.cc/QdBvRCw7/oiljoilk.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
